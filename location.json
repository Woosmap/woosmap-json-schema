{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://raw.githubusercontent.com/woosmap/woosmap-json-schema/master/location.json#",
  "title": "Woosmap Location",
  "description": "A geographical coordinate for an Asset",
  "type": "object",
  "required": [
    "lat",
    "lng"
  ],
  "properties": {
    "lat": {
      "type": "number"
    },
    "lng": {
      "type": "number"
    }
  },
  "additionalProperties": false
}